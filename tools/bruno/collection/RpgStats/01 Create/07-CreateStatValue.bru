meta {
  name: 07-CreateStatValue
  type: http
  seq: 7
}

post {
  url: {{host}}/{{basePath}}/{{statValuePath}}/CreateStatValue
  body: json
  auth: none
}

body:json {
  {
    "level": 1,
    "value": 100,
    "containedBonusNum": 5,
    "containedBonusPercent": 2,
    "statId": 2,
    "characterId": 1
  }
}

vars:post-response {
  statValueId: res.body.data.id
}

assert {
  res.status: eq 201
}
